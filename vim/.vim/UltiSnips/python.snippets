snippet #! "#!/usr/bin/env python3" b 
#!/usr/bin/env python3
$0
endsnippet

snippet #- "encoding utf-8" b
# -*- coding:utf-8 -*-
$0
endsnippet

snippet fr "From some import some" b
from $1 import $2
$0
endsnippet

snippet im "Import some" b
import $1
$0
endsnippet

snippet [ "Insert brackets"
[${1:something}]
$0
endsnippet

snippet ( "Insert parenthesis" 
($1)
$0
endsnippet
